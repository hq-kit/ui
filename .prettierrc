{
    "plugins": ["prettier-plugin-tailwindcss", "@trivago/prettier-plugin-sort-imports"],
    "tailwindFunctions": ["tv", "cn", "twMerge"],
    "printWidth": 120,
    "importOrder": ["^react$", "<THIRD_PARTY_MODULES>", "^[@/]", "^[./]"],
    "importOrderSeparation": true,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "trailingComma": "none",
    "tabWidth": 4,
    "semi": false,
    "useTabs": false,
    "bracketSpacing": true,
    "bracketSameLine": false
}
