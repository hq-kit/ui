{
    "tailwindFunctions": ["twJoin", "clsx", "tw", "tv", "cn", "twMerge", "tm", "cx"],
    "plugins": ["@trivago/prettier-plugin-sort-imports", "prettier-plugin-tailwindcss"],
    "printWidth": 100,
    "importOrder": ["^react$", "<THIRD_PARTY_MODULES>", "^[@/]", "^[./]"],
    "importOrderSeparation": true,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "trailingComma": "none",
    "tabWidth": 4,
    "semi": false,
    "useTabs": false,
    "bracketSpacing": true,
    "bracketSameLine": false
}
